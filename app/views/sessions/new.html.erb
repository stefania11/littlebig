<h1>Log in</h1>
<%= form_tag sessions_path do %>
  <fieldset>
    <div class="field">
      <%= label_tag :email %><br />
      <%= text_field_tag :email, params[:email] %>
    </div>
  </fieldset>

  <div class="field">
    <%= label_tag :password%>
    <%= password_field_tag "params[:password]" %>
  </div>

  <div class="actions">
    <%= submit_tag "Log in" %>
  </div>

<h1>Sign up</h1>
<%= form_tag 'sign-up' do %>
  <fieldset>
    <div class="">
      <%= label_tag "Username"%>
      <%= text_field_tag "user[username]" %>
    </div>
  </fieldset>

  <div class="">
    <%= label_tag "Password"%>
    <%= text_field_tag "user[password]" %>
  </div>

  <div class="">
    <%= label_tag "Password"%>
    <%= password_field_tag "user[password]" %>
  </div>
